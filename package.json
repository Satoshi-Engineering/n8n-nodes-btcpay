{
  "name": "@satoshiengineering/n8n-nodes-btcpay",
  "version": "0.1.11",
  "private": false,
  "description": "BtcPay integration for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Satoshi Engineering",
    "email": "hello@satoshiengineering.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Satoshi-Engineering/n8n-nodes-btcpay.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "test": "jest",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "bump-version": "pnpm run lint && npx changelogen@latest --release && git push --follow-tags"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BtcPayApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/BtcPay/BtcPay.node.js",
      "dist/nodes/BtcPay/BtcPayTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "express": "^4.21.2",
    "get-tsconfig": "^4.10.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "jest-mock-extended": "4.0.0-beta1",
    "prettier": "^3.3.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-workflow": "*"
  },
  "pnpm": {
    "overrides": {
      "decode-uri-component@<0.2.1": ">=0.2.1",
      "es5-ext@>=0.10.0 <0.10.63": ">=0.10.63",
      "braces@<3.0.3": ">=3.0.3",
      "micromatch@<4.0.8": ">=4.0.8",
      "cross-spawn@>=7.0.0 <7.0.5": ">=7.0.5",
      "semver@<5.7.2": ">=5.7.2",
      "@babel/helpers@<7.26.10": ">=7.26.10",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
      "form-data@>=4.0.0 <4.0.4": ">=4.0.4",
      "axios@>=1.0.0 <1.12.0": ">=1.12.0"
    }
  }
}
